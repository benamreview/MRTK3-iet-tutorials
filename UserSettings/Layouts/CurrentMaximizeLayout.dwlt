  - m_Type: 6
    Title:
      m_Untranslated: 
    Text:
      m_Untranslated: 
    m_Tutorial: {fileID: 0}
    m_Image: {fileID: 0}
    m_Video: {fileID: 0}
    m_CriteriaCompletion: 0
    m_Criteria:
      m_Items: []
    m_MaskingSettings:
